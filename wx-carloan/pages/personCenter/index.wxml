        </view>
        <!--  -->
        <view class='cellBox'> 
        <navigator url="/pages/myMsg/index"   plain='true'>
          <div class="cellLine">
            <span class="cellIcon"><image src='http://creditsit.huaxiafinance.com/huaxiapuhui/markdownPic/personcenter10.png'></image></span>
            <span class="cellTitle">我的消息</span>
            <span class="cellText"></span>
