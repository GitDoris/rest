            <span class="cellTitle">当前版本</span>
            <span class="cellText">v1.0.0</span>
            <span class="cellNav"></span>
          </div>
        </view>

        <button  size="{{warnSize}}" loading="{{loading}}" plain="{{plain}}"
		disabled="{{disabled}}" class='AClinkBtn'  bindtap="SignOut"  > 退出登录 </button>
    </view>  
</view> 